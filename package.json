{
  "name": "thorax",
  "version": "1.3.0",
  "description": "Strengthening your Backbone",
  "keywords": ["mobile", "backbone"],
  "authors": [
    "Ryan Eastridge <ryan@syntacticx.com> (http://syntacticx.com)",
    "Kevin Decker <kpdecker@gmail.com> (http://incaseofstairs.com)"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/walmartlabs/thorax.git"
  },
  "engines": {
    "node": ">=0.4.7"
  },
  "dependencies": {
    "connect": "2.3.3",
    "underscore": "1.3.3",
    "mkdirp": "0.3.3",
    "async": "0.1.22",
    "fs-watch-tree": "0.2.0",
    "uglify-js": "1.3.2"
  },
  "scripts": {
    "start": "node bin/server.js . 8083",
    "test": "phantomjs bin/run-qunit.js 'http://localhost:8083/zepto.html' 'http://localhost:8083/jquery.html' 'http://localhost:8083/zepto-form.html' 'http://localhost:8083/jquery-form.html' 'http://localhost:8083/zepto-data.html' 'http://localhost:8083/jquery-data.html'"
  },
  "builds": {
    "thorax": {
      "sources": [
        "packages/base/js/src/header.js",
        "packages/base/js/src/constants.js",
        "packages/base/js/src/util.js",
        "packages/base/js/src/view.js",
        "packages/base/js/src/view/mixin.js",
        "packages/base/js/src/view/events.js",
        "packages/base/js/src/view/model.js",
        "packages/base/js/src/view/render.js",
        "packages/base/js/src/view/partial.js",
        "packages/base/js/src/view/helpers.js",
        "packages/base/js/src/view/helpers/view.js",
        "packages/base/js/src/view/helpers/collection.js",
        "packages/base/js/src/view/helpers/empty.js",
        "packages/base/js/src/view/events-builtin.js",
        "packages/base/js/src/view/scroll.js",
        "packages/base/js/src/layout.js",
        "packages/base/js/src/router.js",
        "packages/base/js/src/view-controller.js",
        "packages/base/js/src/application.js",
        "packages/base/js/src/model.js",
        "packages/base/js/src/collection.js",
        "packages/base/js/src/export.js",
        "packages/base/js/src/registry.js",
        "packages/base/js/src/$.js",
        "packages/base/js/src/footer.js"
      ],
      "target": "builds/base/js/lib/thorax.js"
    },
    "thorax.data": {
      "sources": [
        "packages/data/js/src/data.js",
        "packages/data/js/src/helpers.js"
      ],
      "target": "builds/base/js/lib/thorax.data.js"
    },
    "thorax.form": {
      "sources": [
        "packages/form/js/src/form.js",
        "packages/form/js/src/validation.js",
        "packages/form/js/src/helpers.js"
      ],
      "target": "builds/base/js/lib/thorax.form.js"
    },
    "thorax.combined": {
      "sources": [
        "packages/base/js/lib/jquery.js",
        "packages/base/js/lib/underscore.js",
        "packages/base/js/lib/backbone.js",
        "packages/base/js/lib/handlebars.js",
        "packages/bootstrap/js/lib/bootstrap.js",
        "builds/base/js/lib/thorax.js",
        "builds/base/js/lib/thorax.data.js",
        "builds/base/js/lib/thorax.form.js"
      ],
      "target": "builds/thorax.combined.js",
      "uglify": true
    },
    "thorax-html": {
      "directories": {
        "packages/boilerplate-html": "",
        "packages/base/js/lib": "js/lib",
        "builds/base/js/lib": "js/lib",
        "packages/bootstrap": ""
      },
      "after": "rm builds/thorax-html/lumbar.json"
    },
    "boilerplate-lumbar": {
      "files": {
        "packages/boilerplate-lumbar/lumbar.json": "lumbar.json",
        "packages/form/lumbar.json": "lumbar.json",
        "packages/data/lumbar.json": "lumbar.json",
        "packages/bootstrap/lumbar.json": "lumbar.json"
      },
      "directories": {
        "packages/boilerplate-lumbar": "",
        "builds/base/js/lib": "js/lib",
        "packages/base/js/lib": "js/lib",
        "packages/bootstrap": "",
        "packages/data/js/views": "js/views",
        "packages/data/styles": "styles",
        "packages/data/templates": "templates"
      }
    },
    "thorax-node": {
      "builds": ["boilerplate-lumbar"],
      "directories": {
        "packages/hello-world": "",
        "packages/admin": "",
        "packages/base/js/lib": "public/admin/js/lib",
        "builds/base/js/lib": "public/admin/js/lib"
      }
    },
    "thorax-test": {
      "directories": {
        "packages/base/js/lib": "js/lib",
        "builds/base/js/lib": "js/lib",
        "packages/test": "",
        "packages/base/js/lib": "js/lib"
      }
    }
  }
}

{
  "name": "thorax",
  "version": "1.3.0",
  "description": "Strengthening your Backbone",
  "keywords": ["mobile", "backbone"],
  "author": "Ryan Eastridge <ryan@syntacticx.com> (http://syntacticx.com)",
  "repository": {
    "type": "git",
    "url": "git://github.com/walmartlabs/thorax.git"
  },
  "engines": {
    "node": ">=0.4.7"
  },
  "dependencies": {
    "connect": "2.3.3",
    "underscore": "1.3.3",
    "mkdirp": "0.3.3",
    "async": "0.1.22",
    "fs-watch-tree": "0.2.0"
  },
  "scripts": {
    "start": "node bin/server.js . 8083",
    "test": "phantomjs bin/run-qunit.js 'http://localhost:8083/zepto.html' 'http://localhost:8083/jquery.html'"
  },
  "builds": {
    "base": {
      "lib/thorax.js": "thorax.js"
    },
    "boilerplate-html": {
      "files": {
        "static/boilerplate-html": "",
        "lib": "js/lib"
      }
    },
    "boilerplate-lumbar": {
      "files": {
        "static/boilerplate-lumbar": "",
        "lib": "js/lib"
      }
    },
    "hello-world-lumbar": {
      "builds": ["boilerplate-lumbar"],
      "files": {
        "static/hello-world": ""
      }
    },
    "kitchen-sink": {
      "builds": ["boilerplate-lumbar", "hello-world-lumbar"],
      "files": {
        "static/bootstrap": "",
        "static/loading": "",
        "static/admin": ""
      }
    },
    "test": {
      "files": {
        "static/test": "",
        "lib": "js/lib"
      }
    }
  }
}
